<!DOCTYPE html>
<html>
  <head>
    <title>GameOfThrones</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div class="header">
        <h1><%= link_to "Game of Thrones", houses_path %> </h1>
        <nav>
          <h2><%= link_to "All Houses", houses_path %> |  <%= link_to "Create New House", new_house_path %> </h2>
        </nav>
      </div>
      <%= yield %>
    </div>
  </body>
</html>
